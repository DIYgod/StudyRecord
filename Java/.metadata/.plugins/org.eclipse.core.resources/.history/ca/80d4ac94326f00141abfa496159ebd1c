import java.awt.*;

public class TestPanel {
	public static void main(String[] args)
	{
		PanelCenter pc = new PanelCenter("Anotherhome", 400, 300, 400, 300, Color.orange, Color.gray);
	}
}


class PanelCenter extends Frame
{
	private Panel p;
	PanelCenter(String name, int x, int y, int w, int h, Color framecolor, Color panelcolor)
	{
		super(name);
		p.setBounds(3 / 8 * w, 3 / 8 * h, w / 4, h / 4);
		p.setBackground(panelcolor);
		add(p);
		setBounds(x, y, w, h);
		setVisible(true);
	}
}