import java.net.*;
import java.io.*;

public class ChatServer {
	public static void main(String[] args)
	{
		ServerSocket ss = null;
		try {
			ss = new ServerSocket(1207);
		} catch (IOException e) {
			System.out.println("Can't listen to port " + ss.getLocalPort());
			e.printStackTrace();
		}
		
	}
}
